{
    "name": "galihazizif/mutasi-bni",
    "type": "library",
    "description": "Cek mutasi internet banking BNI",
    "keywords": [
        "galihazizif",
        "mutasi-bni"
    ],
    "homepage": "https://github.com/galihazizif/mutasi-bni",
    "license": "MIT",
    "authors": [
        {
            "name": "Galih Azizi F",
            "email": "galih@rempoah.com",
            "homepage": "rempoah.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "galihazizif\\mutasi-bni\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
